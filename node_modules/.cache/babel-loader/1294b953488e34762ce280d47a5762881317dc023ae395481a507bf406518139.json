{"ast":null,"code":"import{useEffect}from\"react\";import{useSelector}from\"react-redux\";import{Howl}from\"howler\";export var useBGM=function useBGM(url,isStart){var level=arguments.length>2&&arguments[2]!==undefined?arguments[2]:0.5;var isMute=useSelector(function(store){return store.isMute.value;});useEffect(function(){if(isMute)return;var sound=new Howl({src:[url],volume:level,Loop:true});if(!isStart){sound.play();}return function(){sound.stop();};},[url,isStart,level,isMute]);};","map":{"version":3,"names":["useEffect","useSelector","Howl","useBGM","url","isStart","level","arguments","length","undefined","isMute","store","value","sound","src","volume","Loop","play","stop"],"sources":["/Users/Pajamas/portfolios/tetris/tetris-v1/tetris/src/hooks/tetris/useBGM.js"],"sourcesContent":["import { useEffect } from \"react\";\n\nimport { useSelector } from \"react-redux\";\n\nimport { Howl } from \"howler\";\n\nexport const useBGM = (url, isStart, level=0.5) => {\n    const isMute = useSelector((store) => store.isMute.value);\n    useEffect(() => {\n        if(isMute) return;\n\n        const sound = new Howl({\n            src: [url],\n            volume: level,\n            Loop: true,\n        });\n        if(!isStart) {\n            sound.play();\n        }\n\n        return () => {\n            sound.stop();\n        }\n    }, [url, isStart, level, isMute])\n}"],"mappings":"AAAA,OAASA,SAAS,KAAQ,OAAO,CAEjC,OAASC,WAAW,KAAQ,aAAa,CAEzC,OAASC,IAAI,KAAQ,QAAQ,CAE7B,MAAO,IAAM,CAAAC,MAAM,CAAG,QAAT,CAAAA,MAAMA,CAAIC,GAAG,CAAEC,OAAO,CAAgB,IAAd,CAAAC,KAAK,CAAAC,SAAA,CAAAC,MAAA,IAAAD,SAAA,MAAAE,SAAA,CAAAF,SAAA,IAAC,GAAG,CAC1C,GAAM,CAAAG,MAAM,CAAGT,WAAW,CAAC,SAACU,KAAK,QAAK,CAAAA,KAAK,CAACD,MAAM,CAACE,KAAK,GAAC,CACzDZ,SAAS,CAAC,UAAM,CACZ,GAAGU,MAAM,CAAE,OAEX,GAAM,CAAAG,KAAK,CAAG,GAAI,CAAAX,IAAI,CAAC,CACnBY,GAAG,CAAE,CAACV,GAAG,CAAC,CACVW,MAAM,CAAET,KAAK,CACbU,IAAI,CAAE,IACV,CAAC,CAAC,CACF,GAAG,CAACX,OAAO,CAAE,CACTQ,KAAK,CAACI,IAAI,CAAC,CAAC,CAChB,CAEA,MAAO,WAAM,CACTJ,KAAK,CAACK